{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": ".",
  "installCommand": "",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://bankim-nodejs-api-production.up.railway.app/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/mainapp/build/assets/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/mainapp/build/static/$1"
    },
    {
      "src": "/locales/(.*)",
      "dest": "/mainapp/build/locales/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/mainapp/build/favicon.ico"
    },
    {
      "src": "/app$",
      "dest": "/mainapp/build/index.html"
    },
    {
      "src": "/app/(.*)",
      "dest": "/mainapp/build/index.html"
    },
    {
      "src": "/admin$",
      "dest": "/admin.html"
    },
    {
      "src": "/customer$",
      "dest": "/customer-approval-check.html"
    },
    {
      "src": "/personal-admin$",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard$",
      "dest": "/index.html"
    },
    {
      "src": "/$",
      "dest": "/landing.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
} 