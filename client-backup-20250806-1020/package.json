    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "@reduxjs/toolkit": "^1.9.2",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "formik": "^2.2.9",
    "yup": "^1.0.0",
    "classnames": "^2.3.2",
